{
  "name": "discord.js-plugins",
  "version": "0.1.0-dev",
  "description": "Discord.js extension that allows the user to load/unload/reload plugins",
  "license": "Apache-2.0",
  "author": "Robin Dahlgren <admin@opposite.biz>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NbOpposite/discord.js-plugins.git"
  },
  "bugs": {
    "url": "https://github.com/NbOpposite/discord.js-plugins/issues"
  },
  "keywords": [
    "pluginmanager",
    "discord",
    "discord.js"
  ],
  "scripts": {
    "test": "npm run lint && npm run docs:test",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "docs": "docgen --source src --custom docs/index.yml --output docs/docs.json",
    "docs:test": "docgen --source src --custom docs/index.yml"
  },
  "main": "src/index",
  "types": "./typings/index.d.ts",
  "dependencies": {
    "require-all": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^10.5.1",
    "discord.js-docgen": "discordjs/docgen",
    "eslint": "^5.0.1",
    "typescript": "^2.9.2"
  },
  "peerDependencies": {
    "discord.js": "^12.0.0",
    "discord.js-commando": "^0.11.0"
  },
  "engines": {
    "node": ">=8.6.0"
  }
}
